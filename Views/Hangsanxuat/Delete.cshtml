@model QuanLySanXuat.Entities.Hangsx


@{
    Layout = null;

}






<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="Idhsx" />
<div class="row mb-3">
    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Mã sản xuất</label>
        <div>
            @Model.Mahsx 
        </div>
    </div>

    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Tên hãng sản xuất</label>
        <div>
         @Model.Tenhsx
        </div>
    </div>


</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Idhsx" />



    <input type="submit" value="Xác nhận" class="btn btn-danger w-100" />
</form>


