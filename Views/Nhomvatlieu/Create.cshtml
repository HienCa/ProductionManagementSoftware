@model QuanLySanXuat.Entities.Nhomvl
@using QuanLySanXuat.Entities;


@{
    Layout = null;

}




<form asp-action="Create" enctype="multipart/form-data" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="row">
        <div class="form-group col-md-12">

            <label asp-for="Manvl" class="control-label">Mã nhóm vật liệu</label>
            <input asp-for="Manvl" class="form-control form-control-lg" />
            <span asp-validation-for="Manvl" class="text-danger"></span>
        </div>
        <div class="form-group col-md-12">

            <label asp-for="Tennvl" class="control-label">Tên nhóm vật liệu</label>
            <input asp-for="Tennvl" class="form-control form-control-lg" />
            <span asp-validation-for="Tennvl" class="text-danger"></span>
        </div>
        <div class="form-group col-md-12">

            <label asp-for="Loainvl" class="control-label">Loại nhóm vật liệu</label>
            <input asp-for="Loainvl" class="form-control form-control-lg" />
            <span asp-validation-for="Loainvl" class="text-danger"></span>
        </div>



    </div>

    <div class="text-center mt-3">
        <button class="btn btn-success w-100" type="submit">Thêm</button>

    </div>

</form>

