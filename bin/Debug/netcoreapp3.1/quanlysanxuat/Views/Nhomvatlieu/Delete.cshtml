

@model QuanLySanXuat.Entities.Nhomvl


@{
    Layout = null;

}




<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="Idnvl" />
<div class="row mb-3">
    <div class="form-group col-md-8 col-sm-12">

        <label class="control-label">Nhóm vật liệu</label>
        <div>
            @Model.Manvl - @Model.Tennvl
        </div>
    </div>

    <div class="form-group col-md-4 col-sm-12">

        <label class="control-label">Loại vật liệu</label>
        <div>
            @Model.Loainvl
        </div>
    </div>


</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Idnvl" />



    <input type="submit" value="Xác nhận" class="btn btn-danger w-100" />
</form>



