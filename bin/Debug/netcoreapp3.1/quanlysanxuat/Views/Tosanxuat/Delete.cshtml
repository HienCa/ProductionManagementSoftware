@model QuanLySanXuat.Entities.Tosanxuat


@{
    Layout = null;

}



<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="Idgdsx" />
<div class="row mb-3">
    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Mã tổ sản xuất</label>
        <div>
            @Model.Matsx
        </div>
    </div>
    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Tên tổ sản xuất</label>
        <div>
            @Model.Tentsx
        </div>
    </div>
  
    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Giai đoạn sản xuất</label>
        <div>
            @Model.IdgdsxNavigation.Magdsx-
            @Model.IdgdsxNavigation.Tengdsx
        </div>
    </div>
    <div class="form-group col-md-6 col-sm-12">

        <label class="control-label">Ghi chú</label>
        <div>
            @Model.Ghichu
        </div>
    </div>


</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Idgdsx" />


    <input type="submit" value="Xác nhận" class="btn btn-danger w-100" />
</form>



